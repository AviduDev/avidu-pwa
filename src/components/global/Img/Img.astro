---
import { Image } from "astro:assets";

interface Props {
    src: string;
    alt: string;
    width: number;
    height: number;
}

const { src, alt, width, height } = Astro.props;
---

<Image
	src={src}
	alt={alt}
    width={width}
    height={height}
	style="height: 100%; width: 100%; object-fit: cover;"
/>